#!/bin/bash

SCENARIO=tutorial_lxc_pfsense.xml
VERS='02'

# Move to the upper directory where this script is
cdir=$( dirname $0 )
cd $cdir/..

vnx --pack $SCENARIO \
    --pack-version $VERS \
    --pack-add-files \
        00-readme-tutorial-pfsense.txt \
        config.xml \
        bin \
    $1 $2 $3

